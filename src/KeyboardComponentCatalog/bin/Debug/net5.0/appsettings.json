{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },    
    "Console": {
      "FormatterOptions": {
        "FormatterName": "simple",
        "IncludeScopes": true,
        "TimestampFormat": "[yyyy-MM-dd HH:mm:ss] ",
        "UseUtcTimestamp": true
      }
    }
  },
  "AllowedHosts": "*"
}
